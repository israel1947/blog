{
  "version": 2,
  "builds": [
    {
      "src": "app/angular.json",
      "use": "@vercel/static-build",
      "config": {
        "outputPath": "app/dist/sing-in-google-app"
      }
    },
    {
      "src": "api/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "api/dist/**",
          "api/src/**",
          "api/node_modules/**",
          "api/package.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/dist/main.js"
    },
    {
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      },
      "src": "/(.*)",
      "dest": "app/dist/sing-in-google-app/$1"
    }
  ]
}